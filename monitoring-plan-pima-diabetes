        "X_new = df_new.drop(['Outcome'], axis = 1)\n",
        "\n",
        "from scipy.stats import ks_2samp\n",
        "KS_pvalue = ks_2samp(X, X_new).pvalue\n",
        "\n",
        "KS_results = {\n",
        "    'feature': X.columns,\n",
        "    'p-value': np.round(KS_pvalue, 4),\n",
        "    'data drift': KS_pvalue < 0.05\n",
        "}\n",
        "\n",
        "KS_results_df = pd.DataFrame(KS_results)\n",
        "print(KS_results_df)"
      ],
