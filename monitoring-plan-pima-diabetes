        "from scipy import stats\n",
        "\n",
        "# Store the fitted marginal distributions\n",
        "marginal_dist = {}\n",
        "\n",
        "for col in df.columns[:-1]:  # Exclude 'Outcome' (target)\n",
        "    marginal_dist[col] = stats.norm.fit(df[col])\n",
        "\n",
        "# Check the fitted distributions (mean and std for normal distribution)\n",
        "print(marginal_dist)\n",
        "\n",
        "# Apply CDF transformation to each column\n",
        "uniform_df = pd.DataFrame()\n",
        "\n",
        "for col in df.columns[:-1]:\n",
        "    params = marginal_dist[col]\n",
        "    uniform_df[col] = stats.norm.cdf(df[col], *params)\n",
        "\n",
        "# For the 'Outcome' column, we treat it as binary (no transformation needed)\n",
        "uniform_df['Outcome'] = df['Outcome']\n",
        "\n",
        "from copulas.multivariate import GaussianMultivariate\n",
        "\n",
        "# Fit a Gaussian copula to the uniform data\n",
        "copula = GaussianMultivariate()\n",
        "copula.fit(uniform_df)\n",
        "\n"
      ],
